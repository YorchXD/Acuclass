<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acuclass">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.example.acuclass.data.bd.config.AcuClassApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AcuClass">
        <activity android:name="com.example.acuclass.ui.view.activities.ContenedorNotasActivity"></activity>
        <activity
            android:name="com.example.acuclass.ui.view.activities.MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.acuclass.ui.view.activities.LoginActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.acuclass.ui.view.activities.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>